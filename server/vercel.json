
{
    "version": 2,
    "builds": [
        {
            "src": "vue-client",
            "use": "@vercel/vue"
        },
        {
            "src": "server",
            "use": "@vercel/node"
          }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/"
        }
    ]
}
